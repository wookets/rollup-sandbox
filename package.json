{
  "name": "rollup-sandbox",
  "version": "0.0.1",
  "description": "a playground for rollup",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c",
    "build:answer": "rollup -c rollup.config.answer.js",
    "build:json": "rollup -c rollup.config.json-plugin.js",
    "build:cjs": "rollup src/main.js --format cjs",
    "build:watch": "rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wookets/rollup-sandbox.git"
  },
  "keywords": [
    "rollup"
  ],
  "author": "wookets",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wookets/rollup-sandbox/issues"
  },
  "homepage": "https://github.com/wookets/rollup-sandbox#readme",
  "devDependencies": {
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-json": "2.1.1",
    "rollup-plugin-node-resolve": "3.0.0"
  },
  "dependencies": {
    "the-answer": "1.0.0"
  }
}
